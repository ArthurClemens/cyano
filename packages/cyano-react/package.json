{
  "name": "cyano-react",
  "version": "0.6.8",
  "description": "Takes a base component and returns a React component",
  "main": "dist/cyano-react",
  "module": "dist/cyano-react.module.js",
  "scripts": {
    "build": "node ../../node_modules/npm-run-all/bin/run-s/index.js build:*",
    "build:clean": "rimraf dist/*",
    "build:types": "../../node_modules/typescript/bin/tsc --p tsconfig.gen.json",
    "build:module": "TYPESCRIPT=1 ENTRY=src/index.tsx MODULE=1 MODULE_NAME=cyanoReact ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.ts",
    "build:umd": "TYPESCRIPT=1 ENTRY=src/index.tsx MODULE=0 MODULE_NAME=cyanoReact ../../node_modules/rollup/dist/bin/rollup -c ../../scripts/rollup.config.ts",
    "build:prettier": "../../node_modules/prettier/bin-prettier.js --config ../../.prettierrc.json --write \"./dist/*.{d.ts,module.js}\"",
    "build:size": "size-limit",
    "size:why": "size-limit --why",
    "lint": "eslint --fix --ext .ts ./src",
    "prettier": "../../node_modules/prettier/bin-prettier.js --config ../../.prettierrc.json --write \"./{src,scripts}/**/*.{js,jsx,ts,tsx,ejs}\"",
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/cli.js -u"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist",
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "homepage": "https://github.com/ArthurClemens/cyano",
  "license": "MIT",
  "dependencies": {
    "react-hyperscript": "^3.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/react-hyperscript": "^3.0.5",
    "size-limit": "^4.10.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "size-limit": [
    {
      "path": "dist/cyano-react.js"
    }
  ]
}

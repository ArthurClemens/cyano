{
  "name": "cyano-mithril",
  "version": "0.5.0",
  "description": "Takes a base component and returns a Mithril component",
  "main": "dist/cyano-mithril",
  "module": "dist/cyano-mithril.mjs",
  "scripts": {
    "lint": "eslint ./src",
    "build": "npm run clean && npm run rollup",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "clean": "rm -rf dist/*",
    "size": "gzip -c dist/cyano-mithril.js | wc -c"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "homepage": "https://github.com/ArthurClemens/cyano",
  "license": "MIT",
  "dependencies": {
    "mithril-hooks": "^0.5.2"
  },
  "peerDependencies": {
    "mithril": "2.0.0-rc.4"
  }
}
